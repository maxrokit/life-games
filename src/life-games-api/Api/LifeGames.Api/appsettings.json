{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(LocalDB)\\MSSQLLocalDB;Database=LifeGames;Integrated Security=true;Encrypt=false"
  },
  "Board": {
    "MaxIterationsForFinalState": 10000
  },
  "Cors": {
    "AllowedOrigins": []
  },
  "ForwardedHeaders": {
    "KnownProxies": [],
    "KnownNetworks": []
  },
  "RateLimiting": {
    "PermitLimit": 1000,
    "WindowSeconds": 1
  },
  "AllowedHosts": "*"
}
